

<ul>
	<% @images.each do |image| %>
		<li albumId="#{image['album_id']}">
	  		<h3> 
	  			<%= link_to image['image_id'], { :controller => "home", 
	  										   :action => "list_comments", 
	  										   :album_id => image['album_id'],
	  										   :photo_id => image['photo_id'] },
	  										   :class => 'commentLink',
	  										   :target =>  "_blank" %>
	  		</h3>	
			<img imageId="#{image['photo_id']}" src="<%= image['content'] %>" />
		</li>
	<% end %>
</ul>