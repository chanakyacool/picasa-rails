

<ul>
	<% @images.each do |image| %>
		<li albumId="#{image['albumId']}">
	  		<h3> 
	  			<%= link_to image['imageId'], { :controller => "home", 
	  										   :action => "list_comments", 
	  										   :albumId => image['albumId'],
	  										   :imageId => image['imageId'] },
	  										   :class => 'commentLink',
	  										   :target =>  "_blank" %>
	  		</h3>	
			<img imageId="#{image['imageId']}" src="<%= image['content'] %>" />
		</li>
	<% end %>
</ul>
